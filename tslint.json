{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "object-literal-sort-keys": false,
        "no-console": false,
        "no-unused-expression": true,
        "interface-name": false,
        "variable-name": false,
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "no-string-literal": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}